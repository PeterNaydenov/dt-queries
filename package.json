{
  "name": "@peter.naydenov/dt-queries",
  "description": "Set of DT-toolbox query functions",
  "version": "1.1.1",
  "license": "MIT",
  "author": "Peter Naydenov",
  "main": "./dist/dt-queries.umd.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/dt-queries.esm.mjs",
      "require": "./dist/dt-queries.cjs",
      "default": "./dist/dt-queries.umd.js"
    },
    "./package.json": "./package.json",
    "./dist/*": "./dist/*",
    "./src/*": "./src/*"
  },
  "scripts": {
    "start": "node src/main.js",
    "test": "mocha test",
    "cover": "c8 mocha test",
    "build": "rollup -c"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "c8": "^10.1.3",
    "chai": "^6.0.1",
    "dt-toolbox": "^7.4.4",
    "mocha": "^11.7.2",
    "rollup": "^4.52.0"
  },
  "c8": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "node_modules",
      "test",
      "test-data"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ]
  },
  "keywords": [
    "compare",
    "identical",
    "change",
    "same",
    "different",
    "missing",
    "merge",
    "add",
    "update",
    "overwrite",
    "insert",
    "combine",
    "dt",
    "DT-model"
  ]
}
